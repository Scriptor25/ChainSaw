@[c]atoi(pStr: int8*): int32;
@[c]printf(pFormat: int8*, ?): int32;

@fib(n: int32): int32 {
    if (n <= 0) ret 0;
    if (n == 1) ret 1;
    ret fib(n - 1) + fib(n - 2);
}

@[c]main(argc: int32, ppArgv: int8**): int32 {

    if (argc != 2)
        ret 1;

    n: int32 = atoi(ppArgv[1]);
    result: int32 = fib(n);
    printf("Fib(%d) = %d\n", n, result);

    ret 0;
}
